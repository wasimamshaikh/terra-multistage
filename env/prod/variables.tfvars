Environment = "prod"
REGION = "us-east-2"
INSTANCE_TYPE = "t2.micro"
USER = "ec2-user"
sourcefile = "./env/prod/nginx.sh"
destfile = "/tmp/nginx.sh"
chmodfile = "chmod +x /tmp/nginx.sh"
runfile = "sudo /tmp/nginx.sh"
VPC_CIDR = "10.0.0.0/16"
PUB_SUB_CIDR1 = "10.0.1.0/24"
PUB_SUB_CIDR2 = "10.0.2.0/24"
PRI_SUB_CIDR1 = "10.0.3.0/24"
PRI_SUB_CIDR2 = "10.0.4.0/24"
key_pair_name = "prod-key-pair"